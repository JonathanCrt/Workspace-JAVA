package fr.umlv.geom;

public class Circle  {
	
	private final int center;
	private final int rayon;
	/**
	 * @param center
	 * @param rayon
	 */
	public Circle(int center, int rayon) {
		super();
		this.center = center;
		this.rayon = rayon;
	}
	
	
	@Override
	public String toString() {
		return (this.center + " " + this.rayon);
	}
	
	
	
	public
	
}
