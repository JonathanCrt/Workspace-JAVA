/* 
 * Résultat temps d'éxécution programme en C : 
 * real	0m0,357s
 * user	0m0,114s
 * sys	0m0,012s
 */


public class Pascal {
	
	public static int pascal(int nBut, int pBut) {
		
		int tab[] = {};
		
		int i, n;
		
		
		tab[0] = 1;
		
		for(n=1; n<=nBut; n++){
	         tab[n] = 1;

	         for(i=n-1; i>0; i--)
	           tab[i] = tab[i-1] + tab[i];
	       }
		
		int result =tab[pBut];
		
		return result;
		
	}
	
	
	public static void main(String[] args) {
		
	
		System.out.println(pascal(30000, 250));
		
		
	}
	
}
