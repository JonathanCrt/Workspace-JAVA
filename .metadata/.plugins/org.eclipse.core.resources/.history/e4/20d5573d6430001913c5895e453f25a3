



public class StringToInt {
	
	/* 
	 * statique signifie qu'une méthode peut s'exécuter sans avoir à
		instancier la classe qui la contient. 
		L'appel à une méthode statique se fait en
		utilisant le nom de la classe, plutôt que le nom de l'objet.

	 */
	 public static int tab_int(String tab) {
		 
		int convert =  Integer.parseInt(tab);
		return convert;
		 
	 }
	 
	 public static int tab_sum(int[] tab) {
		 
		 int i,j;
		 
	 }	

	public static void main(String[] args) {
		
		int i;
		
		int sum = 0;
		for(i = 0; i < args.length; i++) {
			tab_int(args[i]);
			
		}
		
				
		
	}
}
