package td1_concurrence;

public class HelloThreadJoin {
	
	private static int  NB_THREADS = 4;
	
	public static void main(String[] args) {
		for(int i = 0; i < NB_THREADS; i++) {
			int id = i;
			new Thread(() -> {
				for(int j = 0; j < 5000; j++) {
					System.out.println("hello " + id + " "+ j);
				}
			}).start();
		}
	}
}
