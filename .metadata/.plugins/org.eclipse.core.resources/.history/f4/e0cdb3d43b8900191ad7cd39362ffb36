package fr.upem.tidy;

public class Accessory extends AbstractItem {
	private final boolean useful;

	/**
	 * @param name
	 * @param material
	 * @param timeToLive
	 * @param useful
	 */
	public Accessory(String name, Material material, int timeToLive, boolean useful) {
		super(name, material, timeToLive);
		this.useful = useful;
	}
	
	
	
	
	
	
}
